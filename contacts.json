{
	"id": "contactsApp",
	"loaderConfig": {
		"paths": {
			"contactsApp": "../contactsApp"
		}
	},
	"dependencies": [
			"contactsApp/ContactsStore",
			"dojox/mobile/Heading",
			"dojox/mobile/ScrollableView",
			"dojox/mobile/ToolBarButton"
	],
	"modules": [
	],
	"controllers": [
		"dojox/app/controllers/Load",
		"dojox/app/controllers/Transition",
		"dojox/app/controllers/Layout"
	],
	"defaultTransition": "slide",
	"defaultView": "list",
	"stores": {
		"contacts": {
			"type": "contactsApp/ContactsStore",
			"params": {
				"data": [
					{
						"id": "1",
						"displayName": "Ed Chatelain",
						"phoneNumbers": [
							{
								"type": "home",
								"value": "10-20-30-40"
							},
							{
								"type": "work",
								"value": "11-22-33-44"
							}
						]
					},
					{
						"id": "2",
						"displayName": "Christophe Jolif",
						"phoneNumbers": [
							{
								"type": "home",
								"value": "11-43-81-63-19"
							},
							{
								"type": "work",
								"value": "11-48-07-13-38"
							}
						]
					}
				]
			}
		}
	},
	"views": {
		"list": {
			"controller": "contactsApp/list",
			"template": "contactsApp/list.html",
			"nls": "contactsApp/nls/list",
			"has": {
				"!phone": {
					"constraint": "left"
				}
			}
		},
		"detail": {
			"controller": "contactsApp/detail",
			"template": "contactsApp/detail.html",
			"nls": "contactsApp/nls/detail"
		}
	},
	"has": {
		"html5history": {
			"controllers": [
				"dojox/app/controllers/History"
			]
		},
		"!html5history": {
			"controllers": [
				"dojox/app/controllers/HistoryHash"
			]
		},
		"!phone": {
			"defaultView": "list+detail"
		}
	}
}
